
.wrap {
	padding: 30rpx;
}
.dev-area {
	display: flex;
	justify-content: space-between;
	flex-wrap: wrap;
}
.dev-card {
	height: 200rpx;
	width: 320rpx;
	border-radius: 30rpx;
	margin-top: 30rpx;
	display: flex;
	justify-content:space-around;
	align-items: center;
	box-shadow: 0 0 15rpx #ccc;
}
.dev-name {
	font-size: 20rpx;
	text-align: center;
	color: #6d6d6d;
}
.dev-log {
	height: 70rpx;
	width: 70rpx;
	margin-top: 10rpx;
}
.dev-data {
	font-size: 50rpx;
	color: #6d6d6d;
}

